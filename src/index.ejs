		<!-- and mainvisual-->
		<!-- start shooting -->
		<section class="p-shooting">
			<div class="l-container">
				<div class="u-contain">
					<img class="u-left" src="<%= getRelativePath('/image/left.png'); %>" alt="「ひるとよる」について">
